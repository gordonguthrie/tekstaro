<h1 id="activate_tekstaro_menu" data-menu="tekstaro_menu_sign_in"><% gettext("Sign in") %></h1>
<!-- <%= username = gettext("username") %> -->
<!-- <%= password = gettext("password") %> -->
<!-- <%= sign_in = gettext("Sign in") %> -->
<%= form_for @conn, Routes.session_path(@conn, :new, @locale), [as: :session], fn f-> %>
  <%= label f, username %>
  <%= text_input(f, :username) %>
  <%= label f, password %>
  <%= password_input(f, :password) %>
  <%= submit(sign_in) %>
<% end %>
